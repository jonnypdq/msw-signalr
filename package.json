{
  "name": "msw-signalr",
  "version": "2.0.1",
  "description": "Adds simple signalR hub mocking to msw",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mamidenn/msw-signalr.git"
  },
  "author": "Martin Dennhardt <martin@dennhardt.de>",
  "license": "MIT",
  "type": "module",
  "engines": {
    "node": ">=18"
  },
  "devDependencies": {
    "@changesets/cli": "^2.27.7",
    "@microsoft/signalr": "^8.0.0",
    "@typescript-eslint/eslint-plugin": "^7.16.1",
    "@typescript-eslint/parser": "^7.16.1",
    "eslint": "^8.56.0",
    "jsdom": "^24.1.0",
    "prettier": "^3.3.3",
    "publint": "^0.2.8",
    "typescript": "^5.5.3",
    "vite": "^5.3.3",
    "vitest": "^2.0.3"
  },
  "dependencies": {
    "mitt": "^3.0.1",
    "msw": "^2.0.0"
  },
  "peerDependencies": {
    "@microsoft/signalr": "^8.0.0"
  },
  "scripts": {
    "build": "vite build",
    "typecheck": "tsc -p ./tsconfig.build.json",
    "prerelease": "npm run lint && npm run build && tsc -p ./tsconfig.build.json && publint",
    "release": "changeset publish",
    "test": "vitest",
    "dev": "vite",
    "lint": "eslint ."
  },
  "files": [
    "./dist"
  ],
  "exports": {
    "types": "./dist/index.d.ts",
    "import": "./dist/msw-signalr.js",
    "require": "./dist/msw-signalr.umd.cjs"
  },
  "keywords": [
    "signalr",
    "api",
    "mock",
    "mocking",
    "worker",
    "prototype",
    "server",
    "service",
    "handler",
    "testing",
    "front-end",
    "back-end"
  ],
  "msw": {
    "workerDirectory": "public"
  }
}
